{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    }
  },
  "JwtSettings": {
    "Issuer": "OrderPaymentSystem",
    "Audience": "OrderPaymentSystem",
    "Authority": "OrderPaymentSystem",
    "Lifetime": 15,
    "RefreshTokenValidityInDays": 7,
    "AccessTokenValidityInMinutes": 10
  },
  "RedisSettings": {
    "InstanceName": "OrderPaymentSystem:Local:"
  },
  "AllowedHosts": "*"
}